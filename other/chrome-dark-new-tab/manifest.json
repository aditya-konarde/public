{
    "name": "Background",
    "permissions" : ["http://*/*", "https://*/*"],
    "version": "1.0",
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["script.js"],
        "all_frames": false,
        "run_at": "document_start"
    }],
    "manifest_version": 2
}
